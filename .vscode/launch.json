{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Aoba-Example",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/App/Aoba-Example",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/App",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Aoba-Example",
            "miDebuggerPath": "/usr/bin/gdb"
        },
